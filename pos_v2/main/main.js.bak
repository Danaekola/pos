function printReceipt(barcodes){
  
  var objectCartItems = new CartItems(barcodes);
  var cartItems=objectCartItems.getCartItems();
  console.log(cartItems);

  var objectCart=new Cart(cartItems);
  var cart = objectCart.allCart();

  var objectPos = new Pos(cart);
  
  var time = new Time();

  var receipt = '***<没钱赚商店>收据***\n' +'打印时间：' + 
	 time.timer() + '\n' + '----------------------\n' +
     objectPos.getItemString() +'----------------------\n' +
	  '挥泪赠送商品：\n'+objectPos.discount()+
    '----------------------\n' +
    '总计：' + objectPos.formatPrice(objectPos.getTotal()) + '(元)\n' +
    '节省：' + objectPos.formatPrice(objectPos.getSave()) + '(元)\n' +
    '**********************';
   console.log(receipt);
}





